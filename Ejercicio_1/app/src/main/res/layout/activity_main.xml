<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".MainActivity">


    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPassword"
            android:ems="10"
            android:id="@+id/editTextTextPassword7"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/editTextTextPersonName2"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintVertical_bias="0.065"
            android:minHeight="48dp" tools:ignore="SpeakableTextPresentCheck"/>
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:text="@string/usuario"
            android:ems="10"
            android:id="@+id/editTextTextPersonName2"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/imageView2" android:layout_marginTop="80dp"
            android:minHeight="48dp"/>
    <ImageView
            android:layout_width="178dp"
            android:layout_height="146dp" app:srcCompat="@drawable/logo" android:id="@+id/imageView2"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginTop="44dp"
            app:layout_constraintHorizontal_bias="0.497"
            android:contentDescription="@string/textLogo"/>
    <Button
            android:text="@string/botonLogin"
            android:layout_width="150dp"
            android:layout_height="48dp" android:id="@+id/button"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/editTextTextPassword7"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintVertical_bias="0.147"
            app:layout_constraintHorizontal_bias="0.103"/>
    <Button
            android:text="@string/botonRegistrarse"
            android:layout_width="149dp"
            android:layout_height="48dp" android:id="@+id/button2"
            app:layout_constraintStart_toEndOf="@+id/button" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/editTextTextPassword7"
            app:layout_constraintHorizontal_bias="0.341" app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintVertical_bias="0.147"/>
</androidx.constraintlayout.widget.ConstraintLayout>